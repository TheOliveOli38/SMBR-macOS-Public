script/source = "extends TilePropertyContainer

func _ready() -> void:
	$Label.text = tile_property_name.replace(\"_\", \" \")
	if has_meta(\"sfx\"):
		add_sfx()
	else:
		for i in values:
			$OptionButton.add_item(values[i].to_upper(), i)

func add_sfx() -> void:
	$OptionButton.clear()
	for i in AudioManager.sfx_library.keys():
		$OptionButton.add_item(i.to_upper())
"

